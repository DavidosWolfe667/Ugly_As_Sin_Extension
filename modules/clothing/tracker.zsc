class UaS_Clothing_Handler : Inventory {
	int basepocketspace;
	property BasePocketSpace: basepocketspace;

	default {
		UaS_Clothing_Handler.BasePocketSpace 10;
	}

	override void DoEffect() {
		if(!owner) { return; }
		HDPlayerPawn owner = HDPlayerPawn(owner);

		owner.maxpocketspace = basepocketspace;
		//console.printf(""..owner.maxpocketspace);
	}
}
